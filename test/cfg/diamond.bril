# RUN: cat %s | bril2json | make-cfg | filecheck %s

# CHECK: digraph G {
# CHECK-NEXT:   entry;
# CHECK-NEXT:   left;
# CHECK-NEXT:   right;
# CHECK-NEXT:   end;
# CHECK-NEXT:   entry -> left;
# CHECK-NEXT:   entry -> right;
# CHECK-NEXT:   left -> end;
# CHECK-NEXT:   right -> end;
# CHECK-NEXT: }
@main {
  a: int = const 47;
  cond: bool = const true;
  br cond .left .right;
.left:
  a: int = const 1;
  jmp .end;
.right:
  a: int = const 2;
  jmp .end;
.end:
  print a;
}
