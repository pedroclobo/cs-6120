# RUN: cat %s | bril2json | make-dom-tree | filecheck %s

# CHECK: digraph G {
# CHECK-NEXT:   "entry";
# CHECK-NEXT:   "loop";
# CHECK-NEXT:   "body";
# CHECK-NEXT:   "then";
# CHECK-NEXT:   "endif";
# CHECK-NEXT:   "exit";
# CHECK-NEXT:   "entry" -> "loop";
# CHECK-NEXT:   "loop" -> "body";
# CHECK-NEXT:   "body" -> "then";
# CHECK-NEXT:   "body" -> "endif";
# CHECK-NEXT:   "loop" -> "exit";
# CHECK-NEXT: }
@main {
.entry:
  x: int = const 0;
  i: int = const 0;
  one: int = const 1;

.loop:
  max: int = const 10;
  cond: bool = lt i max;
  br cond .body .exit;

.body:
  mid: int = const 5;
  cond: bool = lt i mid;
  br cond .then .endif;

.then:
  x: int = add x one;
  jmp .endif;

.endif:
  factor: int = const 2;
  x: int = mul x factor;

  i: int = add i one;
  jmp .loop;

.exit:
  print x;
}
